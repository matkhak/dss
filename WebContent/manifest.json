{
    "sap.ui5": {
        "rootView": "view.app",
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewPath": "view",
                "viewType": "XML",
                "controlId": "appID",
                "clearTarget": "true",
                "controlAggregation": "pages"
            },
            "routes": [{
                "pattern": "",
                "name": "init",
                "target": "init"
            },{
                "pattern": "chart",
                "name": "chart",
                "target": "chart"
            }],

            "targets": {
                "init": {
                    "viewName": "init",
                    "viewType": "XML"
                },
               "chart": {
                    "viewName": "chart",
                    "viewType": "XML"
                }
            }
        }
    }
}
