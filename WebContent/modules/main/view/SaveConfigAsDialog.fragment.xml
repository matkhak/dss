<core:FragmentDefinition
        xmlns="sap.m"
        xmlns:core="sap.ui.core"
>
    <Dialog title="Сохранение конфигурации">
        <content>
            <VBox>
                <Label text="Название:"/>
                <Input id="configNameInput"/>
            </VBox>
        </content>
        <buttons>
            <Button text="Сохранить" press="onSaveConfigAs"/>
            <Button text="Отмена" press="onCloseSaveConfigAsDialog"/>
        </buttons>
    </Dialog>
</core:FragmentDefinition>