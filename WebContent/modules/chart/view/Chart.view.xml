<?xml version="1.0" encoding="UTF-8" ?>
<core:View
    controllerName="tepaup.modules.chart.controller.Chart"
    xmlns="sap.m"
    xmlns:viz="sap.viz.ui5"
    xmlns:core="sap.ui.core"
    xmlns:common="sap.suite.ui.commons"
    xmlns:custom="tepaup.controls"
>
  <custom:ExtendedChartVariantContainer
          id="chartCont"
          showFullScreen="true"
          autoAdjustHeight="true"
          height="100%"
  >
      <common:ChartContainerContent>
        <common:content>
          <custom:ExtendedVizContainer
            id="vizContainer"
            enableMorphing="true"
            width="100%"
            height="100%"
            vizTypeChanged="onChangeVizType"
          />
        </common:content>
      </common:ChartContainerContent>

  </custom:ExtendedChartVariantContainer>

</core:View>