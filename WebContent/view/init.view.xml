<mvc:View
    controllerName="controller.init"
    xmlns:l="sap.ui.layout"
    xmlns:u="sap.ui.unified"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:m="sap.m"
    xmlns="sap.suite.ui.commons"
    class="viewPadding">
    <!-- Начиная с API 1.44 этот Shell тоже объявлен устаревшим -->
    <!-- Вот йопт нам делать больше нефиг, как гоняться за их придумками -->
    <!-- Правильно Джоэл писал, что успеха добиваются те компании, которые в меньшей степени зависят от таких вот выдумок больших корпораций -->
    <u:Shell id="myShell" icon="img//sng_new_logo.png">
        <u:headItems>
            <!-- <u:ShellHeadItem tooltip="Configuration" icon="sap-icon://menu2" press="handlePressConfiguration"/> -->
            <!-- <u:ShellHeadItem tooltip="Home" icon="sap-icon://home" visible="false" press="handlePressHome"/> -->
        </u:headItems>
        <!-- <u:headEndItems>
            <u:ShellHeadItem icon="sap-icon://hint" tooltip="Logoff" press="handleLogoffPress"/>
        </u:headEndItems> -->
        <u:user>
            <u:ShellHeadUserItem
                image="sap-icon://person-placeholder"
                username="{session>/userName}"
                showPopupIndicator="false"/>
        </u:user>
        <u:content>
            <m:Panel id="panelApps" width="auto" expandable="true" expand="onPanelExpand" class="sapUiResponsiveMargin">
                <m:headerToolbar>
                    <m:Toolbar height="3rem">
                        <m:Title text="Приложения"/>
                    </m:Toolbar>
                </m:headerToolbar>
                <m:content>
                      </m:content>
            </m:Panel>
           
        </u:content>
    </u:Shell>
</mvc:View>
