<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <SelectDialog
    	id="selectDialogFilter"
    	afterClose="_handleAfterCloseDialog"
        height="100%"
        title="Список объектов"
        class="sapUiPopupWithPadding"
        items="{listTO>/}"
        liveChange="_handleValueHelpAddTOSearch"
        confirm="_handleValueHelpAddTOClose"
        cancel="_handleValueHelpAddTOClose">
        <StandardListItem 
        	iconDensityAware="false" 
        	iconInset="false" 
        	description="{listTO>schema}" 
        	title="{listTO>object}"     />
        
        
    </SelectDialog>
</core:FragmentDefinition>
