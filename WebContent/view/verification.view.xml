                               <core:Title text="Критерий точности"/>
                  
                    			<Label required="true" text="Максимальное значение ошибки" textAlign="End"/>
                               <Input/>
           
                               
           
                </f:content>
            </f:SimpleForm>
                </content>
            </Panel>
        
</mvc:View>